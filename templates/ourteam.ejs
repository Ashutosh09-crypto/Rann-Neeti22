<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Team | Rann-Neeti</title>
  <link rel="stylesheet" href="fonts/icomoon/style.css">

  <link rel="stylesheet" href="/css/bootstrap/bootstrap.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css"
    integrity="sha512-xh6O/CkQoPOWDdYTDqeRdPCVd1SpvCA9XXcUnZS2FmJNp1coAFzvtCN9BmamE+4aHK8yyUHUSCcJHgXloTyT2A=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="stylesheet" href="/css/jquery-ui.css">
  <link rel="stylesheet" href="/css/owl.carousel.min.css">
  <link rel="stylesheet" href="/css/owl.theme.default.min.css">
  <link rel="stylesheet" href="/css/owl.theme.default.min.css">
  <link rel="stylesheet" href="/css/navbar.css">
  <link rel="stylesheet" href="/css/jquery.fancybox.min.css">

  <link rel="stylesheet" href="/css/bootstrap-datepicker.css">

  <link rel="stylesheet" href="fonts/flaticon/font/flaticon.css">

  <link rel="stylesheet" href="/css/aos.css">
  <link rel="stylesheet" href="/css/ourteam.css">
  <link rel="stylesheet" href="/css/style.css">
</head>
<style>
  .site-navbar {
    position: unset;
  }

  .site-section {
    padding: 0px !important;
  }

  .site-section {
    width: 100%;

  }

  .wrapper {
    width: 90%;
    display: flex;
    flex-direction: column;
    justify-content: center;
  }

  .img-fluid {
    width: 100% !important;
    height: 100% !important;
    background-size: cover !important;
  }

  .row {
    align-items: center !important;
    justify-content: center !important;
  }

  hr {
    background-color: white;
  }

  h3 {
    font-size: 18px !important;
    font-weight: bold;
  }
</style>

<body onload="load()">

  <div class="site-section">
    <%- include('partials/preloader.ejs') %>
      <div style="background-color: black;">
        <%- include('partials/navbar.ejs') %>
      </div>
      <h1 class="text-white" style=" font-size: 70px;
    margin: 70px 0px 0px 0px;text-align: center">OUR TEAM</h1>
      <div class="wrapper" style="margin: 35px auto;">
        <hr>
        <% for(let heads in headTitles ) {%>
          <div class="title-section">
            <h2 class="heading m-0 py-1" style="font-size: 28px">
              <%=heads%>
            </h2>
          </div>
          <div class="row">
            <% for(let i=0;i<headTitles[heads].length;i++){%>
              <div>
                <div class="our-team">
                  <div class="picture">
                    <img class="img-fluid" style="object-fit: cover; object-position: center;"
                      src="<%=headTitles[heads][i].image_link%>">
                  </div>
                  <div class="team-content">
                    <h3 class="name">
                      <%=headTitles[heads][i].name%>
                    </h3>
                    <h4 class="title">
                      <%= headTitles[heads][i].title%>
                    </h4>
                    <h4 class="title">
                      <%= headTitles[heads][i].phone%>
                    </h4>
                  </div>
                  <ul class="social">
                    <% if(headTitles[heads][i].instagram !="" ) { %>
                      <li><a href="<%=headTitles[heads][i].instagram%>" target="_blank" class="fa-brands fa-instagram"
                          aria-hidden="true"></a></li>
                      <%}%>

                        <% if(headTitles[heads][i].twitter !="" ) { %>
                          <li><a href="<%=headTitles[heads][i].twitter%>" target="_blank" class="fa-brands fa-twitter"
                              aria-hidden="true"></a>
                          </li>
                          <%}%>

                            <% if(headTitles[heads][i].linkedin !="" ) { %>
                              <li><a href="<%=headTitles[heads][i].linkedin%>" target="_blank"
                                  class="fa-brands fa-linkedin" aria-hidden="true"></a></li>

                              <%}%>
                  </ul>
                </div>
              </div>
              <%}%>
          </div>
          <hr>
          <% } %>
      </div>

  </div>
  <%- include('partials/footer.ejs') %>



    <script src="/js/jquery-3.3.1.min.js"></script>
    <script src="/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="/js/jquery-ui.js"></script>
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/owl.carousel.min.js"></script>
    <script src="/js/jquery.stellar.min.js"></script>
    <script src="/js/jquery.countdown.min.js"></script>
    <script src="/js/bootstrap-datepicker.min.js"></script>
    <script src="/js/jquery.easing.1.3.js"></script>
    <script src="/js/aos.js"></script>
    <script src="/js/jquery.fancybox.min.js"></script>
    <script src="/js/jquery.sticky.js"></script>
    <script src="/js/jquery.mb.YTPlayer.min.js"></script>

    <script src="/js/main.js"></script>


</body>

</html>
  <link rel="stylesheet" href="/css/style.css">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  
  <script type="text/javascript" src="https://code.jquery.com/jquery-1.12.0.min.js"></script>

  <link rel="stylesheet" href="/css/confirm_mediaquery.css">

  <style>
    * {
      margin: 0px;
      padding: 0px;
      font-size: 20px;
    }

    #conti {
      width: 40%;
    }
  </style>
  <style>
    @media screen and (max-width:1150px) {
      .site-logo {
        position: unset !important;
      }

      h3 {
        font-size: 23px !important;
      }
    }

    * {
      scroll-behavior: smooth;
      padding: 0px;
      margin: 0px;
      color: white;
    }

    .contactbox span {
      margin: 3px;
    }

    #register {

      width: 42%;
      border-radius: 30px;
      font-size: 13.5px;
    }

    #hamburger {
      position: absolute;

    }

    #cont {
      position: absolute;
      right: 10px;
    }

    .sze {
      letter-spacing: unset !important;
      padding: 0px 9px !important;
      color: #fff;
      font-size: 18px !important;
      display: inline-block;
      text-decoration: none !important;
      text-transform: unset !important
    }
  </style>

</head>

<body onload="load()">
  <div class="site-wrap">
    <%- include('partials/preloader.ejs') %>
    <div class="site-mobile-menu site-navbar-target">
      <div class="site-mobile-menu-header">
        <div class="site-mobile-menu-close">
          <span class="icon-close2 js-menu-toggle"></span>
        </div>
      </div>
      <div class="site-mobile-menu-body"></div>
    </div>
    <header class="site-navbar py-2" role="banner" style="position:relative ;">
      <div class="">
        <div class="d-flex align-items-center">
          <div class="site-logo">
            <a href="/">
              <img
                src="https://user-images.githubusercontent.com/111749567/189416480-17f855ab-9343-4ef1-8aa1-1c7311f55e04.png"
                alt="Logo" height="70px" />
            </a>
            <h3>IIT Mandi</h3>
          </div>
          <!-- changed -->
          <div id="cont">
            <nav class="site-navigation text-right" role="navigation">
              <ul class="site-menu main-menu js-clone-nav mr-auto d-none d-lg-block">
                <li class="active">
                  <a href="/" class="nav-link sze">Home</a>
                </li>
                <li><a href="#about" class="nav-link sze">About Us</a></li>
                <li><a href="/events" class="nav-link sze">Events</a></li>
                <li><a href="#sponsers" class="nav-link sze">Sponsors</a></li>
                <li><a href="#contact" class="nav-link sze">Contact</a></li>
                <li><a href="/ourteam" class="nav-link sze">Our Team</a></li>
                <% if(!authenticated){ %>
                  <li>
                    <a id="sign" href="/auth/google"><i class="nav-link fa-brands fa-google sze"></i> SignIn</a>
                  </li>
                  <% }else{ %>
                    <li><a id="sign" href="/profile" class="nav-link sze"> Profile</a></li>
                    <li><a href="/payment" class="nav-link sze">Payments</a></li>
                    <li>
                      <a id="sign" href="/auth/logout" class="nav-link sze"><i class="fa-brands fa-google"></i>
                        Logout</a>
                    </li>
                    <% } %>
                      <!-- <button id="hamburger" class="material-symbols-outlined">menu</button> -->
              </ul>
            </nav>
            <!-- changed -->
            <a href="#"
              class="d-inline-block d-lg-none site-menu-toggle js-menu-toggle text-black float-right text-white"><span
                class="icon-menu h3 text-white"></span></a>
          </div>
        </div>
      </div>
    </header>

    <div class="row" id="team_form">
      <!-- <div id="eventname" style="    width: 40%;
          position: absolute;
          margin: 70px 30px;">
            <p>Rann-Neeti 22</p>
          </div> -->
      <div id="conti">

        <div class="title-section">
          <h1 class="heading">Please Provide Details</h1>
        </div>
        <form action="#/events/joinTeam" method="post">
          <div class="form-group">
            <input type="text" class="form-control" placeholder="Team-Id" name="teamId">
          </div>
          <div class="form-group">
            <input type="text" class="form-control" placeholder="College" name="college">
          </div>
          <!-- <div class="form-group" style="    margin-left: 45px;">
                    <input type="text" class="form-control" placeholder="Degree">
                  </div>
                  <div class="form-group" style="    margin-left: 45px;">
                    <input type="text" class="form-control" placeholder="Specialization">
                  </div> -->
          <div class="form-group">
            <input type="text" class="form-control" placeholder="Phone" name="phone">
          </div>
          <!-- <div class="form-group">
                    <textarea name="" class="form-control" id="" cols="30" rows="10"
                      placeholder="Write something..."></textarea>
                  </div> -->
          <div class="form-group">
            <input type="submit" class="btn btn-primary" style="border-radius: 30px;
                    padding: 6px;
                    width: 35%;
                    font-size: 17px;" value="Proceed">
          </div>
        </form>
      </div>

